{
  "name": "second-portal-server",
  "version": "0.0.0",
  "description": "description",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon",
    "build": "rm -rf dist && tsc",
    "start": "node dist/main.js",
